{
    "editor.tabSize": 2,
    "files.associations": {
        "*.scss": "postcss"
    },
    "cSpell.words": [
        "acagradient",
        "acala",
        "acalart",
        "acaorange",
        "acapunk",
        "acapunks",
        "acapurple",
        "acared",
        "arrayify",
        "fontawesome",
        "fortawesome",
        "parachain",
        "parachains",
        "Polkadot",
        "Roboto",
        "Segoe"
    ],
    "typescript.extension.sortImports.pathSortOrder": [
        "package",
        "relativeUpLevel",
        "relativeDownLevel"
    ],
    "typescript.extension.sortImports.sortMethod": "path",
    "typescript.extension.sortImports.maxLineLength": 80,
    "typescript.extension.sortImports.omitSemicolon": true,
    "omnisharp.organizeImportsOnFormat": true,
    "volar.completion.preferredTagNameCase": "kebab"
}